{
  "name": "eunomia-cbd-int",
  "version": "0.0.1",
  "description": "Eunomia - room scheduling",
  "main": "server.js",
  "scripts": {
    "dev": "node server",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "sh deploy-docs.sh",
    "yarn-upgrade-deps": "yarn upgrade-interactive --latest",
