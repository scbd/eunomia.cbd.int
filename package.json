    "serve-static": "1.15.0"
  },
  "engine": "node >= 8",
  "repository": {
    "type": "git",
    "url": "https://github.com/scbd/eunomia.cbd.int.git"
  },
  "author": "Secretariat of the Convention on Biological Diversity (SCBD)",
  "license": "MIT",
  "engines": {
    "yarn": ">= 1.6.0"
  },
  "resolutions": {
    "debug": "3.1.0",
    "depd": "1.1.2",
    "setprototypeof": "1.1.0",
    "statuses": "1.5.0",
    "http-errors": "1.6.3",
    "moment": "2.22.2",
    "bootstrap": "4.1.2",
    "jquery": "3.3.1"
  },
  "devDependencies": {
    "vitepress": "^1.6.4",
    "vuepress": "^1.9.8"
  }
